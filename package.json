{
  "name": "checkey",
  "version": "0.1.3",
  "description": "Check Keys existence with whitelist, like rails/strong_parameters.",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src/ --out-dir lib/ --source-maps inline",
    "watch": "npm run build -- --watch",
    "mocha": "mocha 'test/*.js' -r babel-polyfill --compilers js:babel-register",
    "standard": "standard",
    "test": "npm run mocha && npm run standard",
    "clean": "rm -rf lib/"
  },
  "keywords": [
    "validate",
    "validation",
    "strong parameters",
    "mass assignment",
    "whitelist"
  ],
  "author": "Sho Hashimoto <hashimoto@shokai.org>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.0.2",
    "mocha": "^3.4.2",
    "standard": "^8.0.0"
  },
  "dependencies": {},
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shokai/checkey.git"
  },
  "bugs": {
    "url": "https://github.com/shokai/checkey/issues"
  },
